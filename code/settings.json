{
  "workbench.settings.editor": "json",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 16,

  "window.zoomLevel": -1,
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.enableMenuBarMnemonics": false,
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "workbench.iconTheme": "seti",
  "workbench.tips.enabled": false,

  "npm.enableScriptExplorer": true,

  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,

  "git.confirmSync": false,
  "git.enableSmartCommit": true,

  "diffEditor.renderSideBySide": false,

  "editor.acceptSuggestionOnEnter": "off",
  "editor.codeLens": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "Hack, Consolas, 'Courier New', monospace",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.renderCharacters": false,

  "editor.quickSuggestions": {
    "comments": false,
    "strings": true,
    "other": true
  },
  "editor.rulers": [80],
  "editor.scrollbar.verticalScrollbarSize": 20,
  "editor.smoothScrolling": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",

  "editor.overviewRulerBorder": false,

  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",

  "[dockerfile]": {
    "editor.tabSize": 4
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },

  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": false
  },

  "[yaml]": {
    "editor.formatOnSave": false
  },

  /*   "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "javascriptreact"
  }, */
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "tsx"
  },
  "emmet.triggerExpansionOnTab": true,

  "explorer.confirmDelete": false,

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "breadcrumbs.enabled": true,

  "workbench.colorCustomizations": {
    "list.activeSelectionBackground": "#404040",
    "list.inactiveFocusBackground": "#404040",
    "[SynthWave '84]": {
      "editorRuler.foreground": "#495495"
    }
  },

  "xmlTools.enableXmlTreeView": true,

  "gitlens.blame.heatmap.enabled": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },

  "eslint.alwaysShowStatus": true,

  // AWS CloudFormation
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join"
  ],

  "workbench.colorTheme": "Monochrome Dark",

  "java.configuration.checkProjectSettingsExclusions": false,

  "javascript.updateImportsOnFileMove.enabled": "always",

  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "typescript.updateImportsOnFileMove.enabled": "always",

  "jest.showCoverageOnLoad": false,
  "jest.autoEnable": false,

  "auto-close-tag.activationOnLanguage": [
    "xml",
    "html",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascriptreact",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
  ],

  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    //    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
